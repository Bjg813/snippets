<?php 

							$query_args = array(
								'posts_per_page'   => -1,
								'meta_key'         => 'team_member_type',
								'meta_value'       => $archive_section['meta_value'],
								'post_type'        => 'team',
								'post_status'      => 'publish',
							);

							$archive_posts = new WP_Query( $query_args );

							if ( $archive_posts->have_posts() ) : 


								echo '<div class="row">';

									while ($archive_posts->have_posts()) : $archive_posts->the_post(); 

										get_template_part('template-parts/card', 'team');

									endwhile;

								echo '</div>';

							endif;


